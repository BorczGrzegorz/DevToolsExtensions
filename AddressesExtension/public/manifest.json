{
  "short_name": "Qaddr",
  "name": "Quick Addresses",
  "version": "3.1.1",
  "permissions": ["storage", "contextMenus"],
  "options_page": "options.html",
  "web_accessible_resources": ["stamp128.png"],
  "icons": {
    "128": "stamp128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["./static/js/background.js"],
    "persistent": true
  }
}
